<div class="col col-md-5">
  <%= form_with model: @category_management,url: category_management_path(@category_management), method: :patch, local:true do |f| %>
    <table  class="table table-borderless">
        <tbody>
          <tr><td><%= f.collection_select :category_id, Category.all, :id, :name,{:prompt => true}, {:class => "form-control"} %></td></tr>
          <tr><td><%= f.select :limit, options_for_select((1..20).to_a), {:prompt => true}, {:class => "form-control"} %></td></tr>
          <tr><td><%= f.submit "limit", class:"btn btn-outline-secondary" %></td></tr>
        </tbody>
    </table>
  <% end %>
</div>