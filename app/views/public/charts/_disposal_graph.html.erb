  <%= high_chart((target + target_id.to_s), high_chart) %>
  <% if target == "color" %>
    <% targets.each do |color| %>
      <%= form_with url: "/items/disposal", method: :get, local: true do |f| %>
        <%= f.hidden_field :color, value: color.name %>
        <%= f.submit color.name, class: "tbox-style border btn btn-light btn-lg btn-block" %>
      <% end %>
    <% end %>
  <% elsif target == "brand" %>
    <% targets.each do |brand| %>
      <%= form_with url: "/items/disposal", method: :get, local: true do |f| %>
        <%= f.hidden_field :brand, value: brand.name %>
        <%= f.submit brand.name, class: "tbox-style border btn btn-light btn-lg btn-block" %>
      <% end %>
    <% end %>
  <% elsif target == "price" %>
    <% targets.each do |price| %>
      <%= form_with url: "/items/disposal", method: :get, local: true do |f| %>
        <%= f.hidden_field :price, value: price %>
        <%= f.submit price, class: "tbox-style border btn btn-light btn-lg btn-block" %>
      <% end %>
    <% end %>
  <% end %>
